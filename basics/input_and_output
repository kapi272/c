#include <stdio.h>

int main() {
    int number;
    char name[50];

    // Input an integer
    printf("Enter an integer: ");
    scanf("%d", &number);

    // Input a string
    printf("Enter your name: ");
    scanf("%s", name);

    // Output the values
    printf("You entered: %d\n", number);
    printf("Your name is: %s\n", name);

    return 0;
}